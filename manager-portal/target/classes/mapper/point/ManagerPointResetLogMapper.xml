<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mctpay.manager.mapper.ManagerPointResetLogMapper">

    <sql id="Base_Column_List">``
		id, member_id, before_reset_point, operator, reset_time</sql>

    <select id="get" resultType="com.mctpay.manager.model.entity.point.ManagerPointResetLogEntity">
        select
        <include refid="Base_Column_List"/>
        from manager_point_reset_log where id = #{value}
    </select>

    <sql id="where">
        <where>
            <include refid="baseWhere"></include>
        </where>
    </sql>

    <sql id="baseWhere">
        <if test="id != null">
            and id = #{id}
        </if>
        <if test="memberId != null">
            and member_id = #{memberId}
        </if>
        <if test="beforeResetPoint != null">
            and before_reset_point = #{beforeResetPoint}
        </if>
        <if test="operator != null">
            and operator = #{operator}
        </if>
        <if test="resetTime != null">
            and reset_time = #{resetTime}
        </if>
    </sql>
</mapper>